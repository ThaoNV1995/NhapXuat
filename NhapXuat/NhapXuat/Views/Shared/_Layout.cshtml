            function GetCount(ddate, iid) {
                amount = ddate //calc milliseconds between dates
                hours = 0;
                mins = 0;
                secs = 0;
                out = "";
                hours = Math.floor(amount / 3600);
                amount = amount % 3600;
                mins = Math.floor(amount / 60);
