@model IEnumerable<NhapXuat.Models.Slide>

@{
    ViewBag.Title = "Index";
    Layout = "~/Areas/Admin/Views/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.TenSlide)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.AnhSlide)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.STT)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.TenSlide)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.AnhSlide)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.STT)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.MaSlide }) |
            @Html.ActionLink("Details", "Details", new { id=item.MaSlide }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.MaSlide })
        </td>
    </tr>
}

</table>
